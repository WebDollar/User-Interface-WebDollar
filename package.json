{
  "name": "@webdollar/user-interface-webdollar",
  "version": "0.0.4",
  "description": "User Interface for WebDollar",
  "main": "index.js",
  "scripts": {
    "open_browser": "node src/open-browser",
    "build_browser": "webpack --config build/webpack.browser-frontend.config.js",
    "build_user_interface": "webpack --config build/webpack.browser.config.js",
    "build": "npm run build_browser && npm run build_user_interface",
    "test": "npm run build_browser && npm run open_browser",
    "dev": "webpack-dev-server --config=./build/webpack.browser-hot-loader.config.js --content-base dist_bundle --hot --inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDollar/User-Interface-WebDollar.git"
  },
  "keywords": [
    "WebDollar",
    "User Interface",
    "UI",
    "UX",
    "User Experience"
  ],
  "author": "WebDollar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebDollar/User-Interface-WebDollar/issues"
  },
  "homepage": "https://github.com/WebDollar/User-Interface-WebDollar#readme",
  "dependencies": {
    "clipboard": "^1.7.1",
    "gsap": "^2.1.3",
    "nosleep.js": "^0.9.0",
    "uuid": "^3.2.1",
    "v-clipboard": "*",
    "vue": "^2.5.13",
    "vue-notification": "^1.3.12",
    "vue-qrcode-component": "^2.1.1",
    "vue-qrcode-reader": "^2.0.1",
    "vue-slider-component": "^2.8.16",
    "vue-style-loader": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-alias": "^1.6.0",
    "compression-webpack-plugin": "^1.1.3",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "file-saver": "*",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "install": "^0.12.2",
    "open-browser-webpack-plugin": "0.0.5",
    "opn": "^5.1.0",
    "source-map": "^0.7.0",
    "vue-loader": "^15.7.0",
    "vueify": "^9.4.1",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.6.0",
